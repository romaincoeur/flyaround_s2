<script type="text/javascript">
    {% if latitude and longitude %}
    var latlng = new google.maps.LatLng({{ latitude }}, {{ longitude }});
    {% else %}
    var latlng = new google.maps.LatLng(48.8575162, 2.3426246);
    {% endif %}
    // paris : 48.8575162	2.3426246
    //objet contenant des propriétés avec des identificateurs prédéfinis dans Google Maps permettant
    //de définir des options d'affichage de notre carte
    {% if zoom %}
    var options = {
        center: latlng,
        zoom: {{ zoom }},
        mapTypeId: google.maps.MapTypeId.HYBRID,
        disableDefaultUI: true
    };
    {% else %}
    var options = {
        center: latlng,
        zoom: 6,
        mapTypeId: google.maps.MapTypeId.TERRAIN,
        disableDefaultUI: true
    };
    {% endif %}

    //constructeur de la carte qui prend en paramêtre le conteneur HTML
    //dans lequel la carte doit s'afficher et les options
    var map = new google.maps.Map(document.getElementById("map"), options);

    //Chargement des icones
    var icon_bridge = "{{ asset('bundles/flyaroundmap/img/geotheme-26.png') }}";
    var icon_monument = "{{ asset('bundles/flyaroundmap/img/geotheme-28.png') }}";
    var icon_panorama = "{{ asset('bundles/flyaroundmap/img/geotheme-27.png') }}";
    var icon_airport = "{{ asset('bundles/flyaroundmap/img/geotheme-29.png') }}";
    var icon_basicfly = "{{ asset('bundles/flyaroundmap/img/basicfly.png') }}";

    var markers = [];
    {% for entity in flies %}
    {% if entity.latitude and entity.longitude %}
    var LatLng = new google.maps.LatLng({{ entity.latitude }},{{ entity.longitude }});
    var markerfly = new google.maps.Marker({
        position: LatLng,
        map: map,
        {% if entity.category=='bridge' %}
        icon: icon_bridge,
        {% elseif entity.category=='monument' %}
        icon: icon_monument,
        {% elseif entity.category=='panorama' %}
        icon: icon_panorama,
        {% else %}
        icon: icon_basicfly,
        {% endif %}
        title: "{{ entity.category }}-{{ entity.id }}"
    });
    markers.push(markerfly);
    google.maps.event.addListener(markerfly, 'click', function() {
        if ($('#flylist-panel').css('marginLeft') == '0px') {
            this.setIcon(icon_basicfly);
            $('#flylist-panel').animate({marginLeft: '-45%'});
        }
        else {
            console.log(this.title);
            var id = this.title.split('-')[1];
            console.log(id);
            this.setIcon(icon_basicfly);
            $.ajax({
                type        : "GET",
                url         : "api/flies/"+id+".json",
                success     : function(response) {
                    console.log(response);
                    $("#flies-latlng").html(response.latitude+" "+response.longitude);
                    $("#flies-phone").html(response.phone);
                    $('#flies-address1').html(response.address);
                    $('#flies-address2').html(response.postCode+" "+response.city);
                    $('#flies-name').html(response.name);
                },
                error     : function(data) {
                    console.log(data);
                }
            });
            $(".panel").each(function (index) {
                $(this).animate({marginLeft: '-45%'});
            });
            $('#flylist-panel').animate({marginLeft: '0'});
        }
    });
    {% endif %}
    {% endfor %}

    {% for terr in terrains %}
    {% if terr.latitude and terr.longitude %}
    LatLng = new google.maps.LatLng({{ terr.latitude }},{{ terr.longitude }});
    marker = new google.maps.Marker({
        position: LatLng,
        map: map,
        icon: icon_airport,
        title: "airport-{{ terr.id }}"
    });
    markers.push(marker);
    google.maps.event.addListener(marker, 'click', function() {
        if ($('#flylist-panel').css('marginLeft') == '0px') {
            this.setIcon(icon_basicfly);
            $('#flylist-panel').animate({marginLeft: '-45%'});
        }
        else {
            console.log(this.title);
            var id = this.title.split('-')[1];
            console.log(id);
            this.setIcon(icon_airport);
            $.ajax({
                type        : "GET",
                url         : "api/terrains/"+id+".json",
                success     : function(response) {
                    console.log(response);
                    $("#terrain-latlng").html(response.latitude+" "+response.longitude);
                    $("#terrain-phone").html(response.phone);
                    $('#terrain-address1').html(response.address);
                    $('#terrain-address2').html(response.postCode+" "+response.city);
                    $('#terrain-name').html(response.name);
                },
                error     : function(data) {
                    console.log(data);
                }
            });
            $(".panel").each(function (index) {
                $(this).animate({marginLeft: '-45%'});
            });
            $('#terrain-panel').animate({marginLeft: '0'});
        }
    });
    {% endif %}
    {% endfor %}

    // Add Marker Clusterer to map
    var mcOptions = {gridSize: 50, maxZoom: 15};
    var mc = new MarkerClusterer(map, markers, mcOptions);

</script>

